{
    "name": "doctrine/rst-parser",
    "description": "PHP library to parse reStructuredText documents",
    "keywords": ["rst", "markup", "parser"],
    "homepage": "https://github.com/doctrine/rst-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©goire Passault",
            "email": "g.passault@gmail.com",
            "homepage": "http://www.gregwar.com/"
        },
        {
            "name": "Jonathan H. Wage",
            "email": "jonwage@gmail.com",
            "homepage": "https://jwage.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/filesystem": "^4.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^4.0",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-deprecation-rules": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpstan/phpstan-strict-rules": "^0.10",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\RST\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\Tests\\RST\\": "tests/"
        }
    }
}
